{
  "manifest_version": 3,
  "name": "Hordie",
  "version": "0.1.0",
  "description": "Horde your tabs for later",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Horde tabs"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabGroups"
  ],
  "minimum_chrome_version": "89"
}